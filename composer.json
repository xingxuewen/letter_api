{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "laravel/lumen-framework": "5.5.*",
        "illuminate/redis": "^5.5",
        "illuminate/mail": "^5.5",
        "illuminate/routing": "^5.5",
        "vlucas/phpdotenv": "~2.2",
        "laravel/passport": "^4.0",
        "predis/predis": "^1.1",
        "guzzlehttp/guzzle": "^6.3",
        "jenssegers/agent": "^2.5",
        "jenssegers/mongodb": "^3.3.1",
        "jpush/jpush": "^3.5",
        "qiniu/php-sdk": "^7.2",
        "barryvdh/laravel-cors": "^0.9",
        "aishan/lumen-captcha": "^1.3",
        "dusterio/lumen-passport": "^0.2",
        "soapbox/laravel-formatter": "^2.0",
        "mnabialek/laravel-sql-logger": "^1.1.3",
        "simplesoftwareio/simple-qrcode": "^1.5",
        "jenssegers/mongodb-sentry": "^1.0",
        "aliyunmq/mq-http-sdk": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
